{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-typescript",
      {
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
